{
	"manifest_version":1,
	"type":"WebApp",
	"meta":{
		"name":"Nowa apka lol",
		"version":"1.0.0",
		"minimum-overwolf-version":"0.77.10",
		"author":"developerzy z Poznania",
		"icon":"IconMouseOver.png",
		"icon_gray":"IconMouseNormal.png",
		"description":"Demo App"
	},
	"data": {
		"hotkeys":{
			"my_cool_action": {
				"title": "My Cool Action",
				"action-type": "custom",
				"default": "Ctrl+F12"}
			},
		"start_window":"MainWindow",
		"windows":{
			"MainWindow":{
					"file":"index.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":800, "height":200},
					"min_size":{"width":100, "height":100},
					"max_size":{"width":1000, "height":1000}                               
				},
			"SubWindow":{
					"file":"subwindow.html",
					"transparent":true,
					"resizable":true,
					"size":{"width":600, "height":600},
					"in_game_only":false
				}
			
			}
			
		}
	}
